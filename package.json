{
  "name": "types-web",
  "version": "0.1.0",
  "description": "Provides TypeScript types for the latest web APIs.",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && node ./lib/index.js",
    "fetch-comment": "tsc -p ./tsconfig.json && node ./lib/fetch-comment.js",
    "fetch-mdn": "npm run build && node ./lib/fetch-mdn.js",
    "fetch": "echo This could take a few minutes... && npm run fetch-comment && npm run fetch-mdn",
    "baseline-accept": "cpx \"generated\\*\" baselines\\",
    "test": "tsc -p ./tsconfig.json && node ./lib/index.js && node ./lib/test.js && node ./unittests/index.js"
  },
  "author": {
    "name": "Kagami Sascha Rosylight",
    "email": "saschanaz@outlook.com",
    "url": "https://github.com/saschanaz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saschanaz/types-web/"
  },
  "license": "Apache-2.0",
  "type": "module",
  "devDependencies": {
    "@mdn/browser-compat-data": "3.0.1",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.7",
    "@types/webidl2": "^23.13.5",
    "browser-specs": "^1.25.0",
    "cpx2": "^3.0.0",
    "jsdom": "^16.4.0",
    "node-fetch": "^2.6.1",
    "print-diff": "^1.0.0",
    "styleless-innertext": "^1.1.2",
    "typescript": "^4.2.0-dev.20201211",
    "webidl2": "^23.13.0",
    "webref": "github:w3c/webref#ae50d8f3cc75f0d247cbb34454751024410777f4"
  },
  "files": [
    "baselines/*"
  ]
}
